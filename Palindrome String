class Solution {
    boolean isPalindrome(String s) {
        int left = 0;
        int right =s.length()-1;
        // code here
        while(left<right){
            if(s.charAt(left)!=s.charAt(right)){
return false;}
left++;
right--;}
return true;}
   
    public static void main(String[]args){
        String s="abba";
        Solution sol =new Solution();
        System.out.println(sol.isPalindrome(s));}}
   
