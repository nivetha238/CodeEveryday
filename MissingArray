public class main{
    public static int missingArray(int[]arr){
        int n=arr.length+1;
        n=n*(n+1)/2;
        int sum=0;
        for(int i=0;i<arr.length;i++){
            sum+=arr[i];}
           int total = n-sum;
           return total;
        }
        public static void main(String[]args){
            int arr[]={1,2,4,5};
            System.out.println(missingArray(arr));
        }
    }
