public class main{
    public static void main(String[]args){
        String s="mam";
        String temp=s;
        String rev="";
        for(int i=s.length()-1;i>=0;i--){
            rev=rev+s.charAt(i);}
            if(temp.equals(rev)){
            System.out.println("palindrome");}
            else{
                System.out.println("Not Palindrome");
            }
        }
        }
